#LAMMPS Input file generated by Avogadro

# Intialization
units          metal
dimension      3
boundary       p p p
atom_style     full

read_data      data.lmpdat
read_dump      conf.xyz 0 x y z box no replace yes format xyz
replicate      1 1 1

pair_style	hybrid/overlay lj/mdf 9.0 12.0 buck/mdf 9.0 12.0 coul/long 9.0
pair_coeff      1 1 lj/mdf 0.006740 3.165492 9.0 12.0
pair_coeff	2 * lj/mdf 0.00000 0.0000 9.0 12.0
pair_coeff	1 2 lj/mdf 0.00000 0.0000 9.0 12.0
pair_coeff      3 3 lj/mdf 0.0000 0.0000 9.0 12.0
pair_coeff      3 4 lj/mdf 0.0000 0.0000 9.0 12.0
pair_coeff	1 3 lj/mdf 0.00095 3.35000  9.0 12.0
pair_coeff      1 4 lj/mdf 0.00000 0.0000 9.0 12.0
pair_coeff	4 4 lj/mdf 0.00000 0.0000 9.0 12.0
pair_coeff      4 5 lj/mdf 0.00000 0.0000 9.0 12.0
pair_coeff	5 5 buck/mdf 103585.02 0.200 0.0 9.0 12.0
pair_coeff	1 5 buck/mdf 12534.455133 0.246 0.0 9.0 12.0
pair_coeff	3 5 buck/mdf 1815.6986 0.283373 0.0 9.0 12.0
pair_coeff	* * coul/long

kspace_style    ewald 1.0e-5

bond_style	hybrid harmonic morse
bond_coeff	1 harmonic 22.965 1.012
bond_coeff      2 morse 5.0 1.2 1.505

angle_style	harmonic
angle_coeff	1 1.64568 113.24
angle_coeff	2 7.50 109.47

dihedral_style	none
improper_style	none
info	coeffs

fix umb all colvars colvar_inp unwrap yes output q_1

# Settings
velocity       all create 300.0 4928459 dist uniform
timestep       0.0005
fix	       1 all temp/csvr 300.0 300.0 0.05 54324
fix            2 all nve


compute 1 all pe/atom

# Output
thermo_style   multi
thermo         100
thermo_modify format float %20.15g

dump           dumpXYZ2 all xyz 100 traj.xyz
dump_modify dumpXYZ2 pbc yes
dump_modify dumpXYZ2 append yes
dump_modify dumpXYZ2 element Ow H Ca S O


# Run the simulation
run           500000
